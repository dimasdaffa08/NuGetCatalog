<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<IsPackable>true</IsPackable>

		<PackageId>NuGetCatalog.PackageVersions</PackageId>
		<Version>1.0.0</Version>
		<Authors>DimasMD</Authors>
		<Description>Centralized Package Version Management</Description>

		<IncludeBuildOutput>false</IncludeBuildOutput>
		<IncludeSymbols>false</IncludeSymbols>
		<SuppressDependenciesWhenPacking>true</SuppressDependenciesWhenPacking>

		<DevelopmentDependency>true</DevelopmentDependency>
		<NoPackageAnalysis>true</NoPackageAnalysis>

		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
		<None Include="build\NuGetCatalog.PackageVersions.props" Pack="true" PackagePath="build\" />
		<None Include="build\NuGetCatalog.PackageVersions.props" Pack="true" PackagePath="buildMultiTargeting\" />
	</ItemGroup>

</Project>
